<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.project.StudentListController">

    <TableView fx:id="studentTable">
        <columns>
            <TableColumn text="Student ID" prefWidth="100" fx:id="idColumn"/>
            <TableColumn text="Student Name" prefWidth="200" fx:id="nameColumn"/>
            <TableColumn text="Attendance" prefWidth="100" fx:id="attendanceColumn"/>
        </columns>
    </TableView>
</VBox>
